      },
      "domain_strategy": "prefer_ipv4",
      "users": [
        {
          "uuid": "1fb38348-70a9-4265-9337-0deaf5e323f7",
          "flow": "",
          "email": "amin@vless"
        }
      ],
      "transport": {
        "type": "ws",
        "path": "/digikala.com/login/?ed=2560",
        "headers": {
          "Host":amin-vless-server-production.up.railway.app
        }
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ]
