      "sniff": true,
      "sniff_override_destination": true,
      "tls": {
        "enabled": true,
        "server_name": "YOUR_DOMAIN",
        "cert": {
          "certificate_file": "/app/fullchain.cer",
          "key_file": "/app/private.key"
